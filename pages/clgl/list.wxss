.car-list-box{
	width: 100vw;
	height: 100vh;
	background-color: #f0f0f0;
	overflow: hidden;
}

.car-pic-box{
	overflow: hidden;
	border-radius: 50%;
	width: 60px;
	height: 60px;
	margin-right: 1rem;
	background-color: #f0f0f0;
}
.car-pic-box.imgcount0{
	text-align: center;
	line-height: 60px;
	color: #999;
	opacity: 0.75;
}
.car-pic-box.imgcount1 > image{
	display: block;
	width: 100%;
	height: 100%;
}
.car-pic-box.imgcount2 > image{
	display: block;
	width: 100%;
	height: 50%;
}